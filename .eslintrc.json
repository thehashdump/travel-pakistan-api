{
    "extends": [
      "airbnb-base"
    ],
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [".js", ".jsx"]
        }
      }
    },
    "overrides": [
      {
        "files": ["*.js", "*.jsx"]
      }
    ],
    "rules": {
      "comma-dangle": 0,
      "no-console": 0,
      "node/prop-types": 0,
      "import/prefer-default-export": 0,
      "class-methods-use-this": 0,
      "import/no-dynamic-require": 0,
      "global-require": 0,
      "camelcase": 0,
      "linebreak-style":0,
      "consistent-return": 0,
      "no-param-reassign": 0,
      "no-tabs": 0,
      "indent": ["error", "tab"],
      "no-shadow": 0,
      "arrow-body-style": 0,
      "no-else-return": 0,
      "operator-linebreak": [
        "error",
        "after",
        {
          "overrides": {
            ":": "before"
          }
        }
      ]
    }
  }